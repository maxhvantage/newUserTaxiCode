<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="0dp"
                android:color="@color/colorbutton" />
            <size
                android:width="50dp"
                android:height="50dp" />
            <solid android:color="@color/colorbutton" />
